{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "dev": "nodemon --ext js,ts,json --watch src --exec 'ts-node' ./src/bin/www",
    "build": " npm link typescript npm run tsc",
    "prebuild": "del './dist'",
    "postbuild": "cpy '**/*' '!**/*.ts' './../dist' --cwd=src --parents",
    "tsc": "tsc"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-routemagic": "^2.0.2",
    "formidable": "^1.2.2",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.29.0",
    "you-are-not": "^0.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/formidable": "^1.0.31",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0"
  }
}
